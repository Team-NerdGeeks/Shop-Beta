<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="vBox" maxHeight="600.0" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="350.0" stylesheets="@../../css/form.css" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.nerdgeeks.shop.Controller.Purchase.ViewPurchasedProductController">
  <children>
      <HBox alignment="CENTER" maxHeight="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="40.0" prefWidth="350.0" styleClass="top-bar" VBox.vgrow="NEVER">
         <children>
            <Label alignment="CENTER" prefWidth="310.0" text="Purchased Product" textFill="WHITE" HBox.hgrow="ALWAYS">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
            <VBox alignment="CENTER_RIGHT" HBox.hgrow="ALWAYS">
               <children>
                  <Button alignment="CENTER_RIGHT" mnemonicParsing="false" onAction="#closeAction" prefHeight="25.0" prefWidth="25.0" styleClass="close-button" />
               </children>
            </VBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
      <ScrollPane fx:id="scrollPane" hbarPolicy="NEVER" VBox.vgrow="ALWAYS">
         <content>
            <VBox prefHeight="320.0" prefWidth="350.0">
               <children>
                  <TableView fx:id="viewPurchasedTable" VBox.vgrow="ALWAYS">
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                  </TableView>
               </children>
            </VBox>
         </content>
      </ScrollPane>
     <HBox alignment="CENTER_RIGHT" maxHeight="-Infinity" prefHeight="40.0" spacing="5.0" styleClass="bottom-bar" VBox.vgrow="ALWAYS">
        <padding>
           <Insets bottom="10.0" left="5.0" right="5.0" top="10.0" />
        </padding>
        <VBox.margin>
           <Insets />
        </VBox.margin>
     </HBox>
  </children>
</VBox>
